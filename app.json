{
  "expo": {
    "name": "Soil Monitoring",
    "slug": "soil-monitoring-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "soilmonitoring",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#27AE60"
    },
    "primaryColor": "#27AE60",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deniswise.soilmonitoring"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#27AE60"
      },
      "package": "com.deniswise.soilmonitoring"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}